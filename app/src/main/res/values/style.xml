<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Widget.Button.Toggle" parent="android:Widget">
        <item name="android:background">@drawable/toggle_bg</item>
    </style>

    <style name="toggleButton">
        <item name="android:buttonStyleToggle">@style/Widget.Button.Toggle</item>
        <item name="android:textColor">@drawable/toogle_text_color</item>
    </style>
    <style name="AddButton">
        <item name="android:background">#00000000</item>
    </style>
    <declare-styleable name="CustomUserCardView">
        <attr name="UserName" format="string"/>
        <attr name="UserId" format="string"/>
        <attr name="UserImage" format="integer"/>
    </declare-styleable>
</resources>